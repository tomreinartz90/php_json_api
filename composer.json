{
    "repositories": [
        {
            "type": "path",
            "url": "./jsonapi/core"
        }
    ],
    "require": {
        "jsonapi/core": "^0.0",
        "slim/slim": "^3.0",
        "catfan/Medoo": "^1.4",
        "firebase/php-jwt": "^4.0"
    },
    "config": {
        "disable-tls": true,
        "secure-http": false
    },
    "autoload": {
        "psr-4": {
            "JsonApi\\": "jsonapi/core"
        }
    }
}
